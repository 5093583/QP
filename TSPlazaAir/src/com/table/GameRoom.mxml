	<s:Label  filters="{[gf01]}" id="labname" x="0" y="4" width="241" color="0xFFFFFF" fontSize="14" textAlign="center"/>

	<s:Label  filters="{[gf01]}" x="10" y="60" width="241" color="#0099CC" id="lab_sell" fontSize="14"/>
	<s:Label filters="{[gf01]}"  x="10" y="32"  width="241" color="#0099CC" id="lab_desc" fontSize="14"/>
	<s:Label  filters="{[gf01]}" x="10" y="88" width="241" color="#0099CC" id="labperson" fontSize="14"/>
	
	
</s:Group>
