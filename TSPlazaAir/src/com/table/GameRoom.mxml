			<s:SolidColor color="0x000000" alpha=".2"/>
		</s:fill>
	</s:Rect>
	
	<s:Rect x="0" y="0" width="241" height="121" radiusX="4" radiusY="4" id="_rectColor1" visible="false">
		<s:fill>
			<s:SolidColor color="0xCCCCCC" alpha=".4"/>
		</s:fill>
	</s:Rect>
