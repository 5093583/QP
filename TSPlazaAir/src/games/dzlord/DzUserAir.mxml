			private function onClickHandler(e : MouseEvent) : void
			{
				switch(e.currentTarget.id)
				{
					case 'KaiShiBtn':
					{
						if(checkPlayed())	return;
						//onDestroyAllDos();
						//						Controller.dispatchEvent(GameEvent.CONTINUE_GAME);
						KaiShiBtn.visible = false;
						Controller.dispatchEvent( 'continueGame' );
					}
				}
			}
			
			private function checkPlayed():Boolean
			{
				if(StaticVars.isTryPlayed && _theModel != null && StaticVars.tryPlayScore < (_theModel.m_wlCellScore*100) )
				{
					Controller.dispatchEvent('gameModuleExit');
					
					MsgManager.getInstance().showMessage1('您的金币不足！请取金币！');
					return true;
				}
				return false;
			}
			
			
			
			
