			public function GameEndShow(lscore:Number,viewCardID:uint) : void
			{
				//				TweenLite.from(KaiShiBtn,1,{delay:2,autoAlpha:1,onComplete:JiXuHandler});
				//				KaiShiBtn.visible = true;
				
				var syView : SyView = new SyView();
				syView.name = 'syView_'+viewCardID;
				
				if(lscore > 0)
				{
					syView.score.text = '+' + TScore.toStringEx(lscore);
					syView.score.setStyle('color',0xFF962D);
//					syView.sy_text.text = '赢';
//					syView.sy_text.setStyle('color',0xFFFF00);
				}
				else if(lscore == 0)
				{
//					syView.score.text = TScore.toStringEx(lscore);
//					syView.score.setStyle('color',0xEEE193);
//					syView.sy_text.text = '平';
//					syView.sy_text.setStyle('color',0xFFFF00);
					syView.visible = false;
					viewCardID = 255;
				}
				else if(lscore < 0)
				{
					syView.score.text = TScore.toStringEx(lscore);
					syView.score.setStyle('color',0xD2E9DF);
//					syView.sy_text.text = '输';
//					syView.sy_text.setStyle('color',0xFF6600);
				}
				addElement(syView);
				if(syAry != null)
					syAry.push(syView);
				
				switch(viewCardID)
				{
					case 0 :{syView.x = 490;	syView.y = 340; break;}
					case 1 :{syView.x = 640;  	syView.y = 320; break;}
					case 2 :{syView.x = 730; 	syView.y = 255; break;}
					case 3 :{syView.x = 730;	syView.y = 160; break;}
					case 4 :{syView.x = 645;	syView.y = 110;  break;}
					case 5 :{syView.x = 340; 	syView.y = 110;  break;}
					case 6 :{syView.x = 235;  	syView.y = 160; break;}
					case 7 :{syView.x = 235;  	syView.y = 255; break;}
					case 8 :{syView.x = 350;  	syView.y = 320; break;}
				}
				
				
				
				//				showContinueBtn();
