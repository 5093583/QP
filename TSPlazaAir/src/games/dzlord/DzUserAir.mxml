			public function GameMessage(_index:int,wViewID:uint = 255):void
			{
				if(_gameRecord == null)
					_gameRecord = [];
				var str:String;
				var wCharId:uint = 255;
				switch(_index)
				{
					case 0://///////////////游戏开始
					{
						str = "游戏开始" + "\n";
						break;
					}
					case 1:
					{
						wCharId = _theModel.ViewChairID(_theModel.m_wDUser);
						str = this["UV_" + wCharId]['IDTxt'].text + " : 庄家 " + "\n";
						break;
					}	
					case 2:
					{
						wCharId = _theModel.ViewChairID(_theModel.m_wBigUser);
						str = this["UV_" + wCharId]['IDTxt'].text + " : 大盲下注 " + TScore.toStringEx(_theModel.m_wlCellScore*2) + "\n";
						break;
					}
					case 3:
					{
						wCharId = _theModel.ViewChairID(_theModel.m_wMaxChipInUser);
						str = this["UV_" + wCharId]['IDTxt'].text + " : 小盲下注 " + TScore.toStringEx(_theModel.m_wlCellScore) + "\n";
						break;
					}
					case 4:
					{
						str = this["UV_" + wViewID]['IDTxt'].text + " : 过牌 " + "\n";
						break;
					}
					case 5:
					{
						str = this["UV_" + wViewID]['IDTxt'].text + " : 跟注 " + TScore.toStringEx(_theModel.m_wlTurnLessScore)+ "\n";
						break;
					}
					case 6:
					{
						str = this["UV_" + wViewID]['IDTxt'].text + " : 加注 " + TScore.toStringEx(_theModel.m_SendScore) + "\n";
						break;
					}
					case 7:
					{
						str = this["UV_" + wViewID]['IDTxt'].text + " : 全下 " + TScore.toStringEx(_theModel.m_wCurrentAddMaxScore) + "\n";
						break;
					}
					case 8:
					{
						str = this["UV_" + wViewID]['IDTxt'].text + " : 弃牌" + "\n";
						break;
					}	
				}
				if(wCharId == 0 || wViewID == 0)
					str = str.substr(5);
				_gameRecord.push(str);
				gameEnd.text = "";
				while(_gameRecord.length > 20)
					_gameRecord.shift();
				gameEnd.text = _gameRecord.join('');
				
				gameEnd.validateNow();
				gameEnd.scroller.verticalScrollBar.value = gameEnd.scroller.verticalScrollBar.maximum;
				gameEnd.validateNow();
				gameEnd.scroller.verticalScrollBar.value = gameEnd.scroller.verticalScrollBar.maximum;
			}
		]]>
