			public function UpdateUserCome(userInfo:tagUserInfoHead, wViewChairID:uint):Boolean
			{
				if(userInfo != null &&( userInfo.dwUserRight != 0 || userInfo.dwMasterRight != 0)) {
					//trace('玩家：'+IDConvert.Id2View(userInfo.dwUserID),userInfo.dwUserRight,userInfo.dwMasterRight);
				}
				var wChaidID:uint = _theModel.ViewChairID(userInfo.wChairID);
				if(_theModel.m_wCurrentPlayerCount == null)
				{
					_theModel.m_wCurrentPlayerCount = new Array();
				}
				_theModel.m_wCurrentPlayerCount.push(userInfo.wChairID);
				
				
				if(wViewChairID < DZFor_9CMDconst.GAME_PLAYER)			//如果用户座位ID不为0
				{
					var user : *;
					if(wChaidID == 0)
					{
						user = this['UV_' + wChaidID] as UserMyself;
					}
					else if(wChaidID == 2 || wChaidID == 3)
					{
						user = this['UV_' + wChaidID] as UserLeft;
					}
					else if(wChaidID == 4 || wChaidID == 5 || wChaidID == 1 || wChaidID == 8)
					{
						user = this['UV_' + wChaidID] as UserTop;
					}
					else if(wChaidID == 6 || wChaidID ==7)
					{
						user = this['UV_' + wChaidID] as UserLeft;
					}
					//		----------------------------		
					if(user != null) 
					{
						if( userInfo.dwUserRight!=0 || userInfo.dwMasterRight!=0 )
						{
							user.userID = userInfo.dwUserID;
							DeZhouModel._getInstance().m_PlayerInfo[wChaidID] = userInfo.dwUserID;
						}else{
							
							user.userID = IDConvert.Id2View(userInfo.dwUserID);
							//							DeZhouModel._getInstance().m_PlayerInfo[wViewChairID] = IDConvert.Id2View(userInfo.dwUserID);
							DeZhouModel._getInstance().m_PlayerInfo[wChaidID] = IDConvert.Id2View(userInfo.dwUserID);
						}
						DeZhouModel._getInstance().m_playerStatus[wChaidID] = 1;
						user.IP = userInfo.ip;
						//						user.Face = userInfo.wFaceID;
						user.Face = userInfo.cbGender;
						user.Offlie(0);
						user.visible = true;
						if(wChaidID == 0) 
						{ 
							user.JinBi = _user.GetSelfData().UserScoreInfo.lScore; 
						}else
						{
							user.JinBi = userInfo.UserScoreInfo.lScore; 
						}
					}
				}
				return true;
			}
			
