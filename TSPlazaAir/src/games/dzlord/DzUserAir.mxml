			public function UpdateUserStatus(userID:int, cbStatus:uint, wTableID:int, wViewChairID:uint):Boolean
			{
				if(cbStatus < TConst.US_SIT || cbStatus == TConst.US_LOOKON || wViewChairID == TConst.INVALID_CHAIR ) 
				{
					if(GlobalModel( GlobalModel.GetInstance() ).m_bReadyGame && _user.count == 1) {
						Controller.dispatchEvent('ready_leave');
						return true;
					}
				}
				
				var i : uint = 0;
				
				if(cbStatus == TConst.US_FREE)// && wViewChairID != 0)
				{
					var wChaidID:uint;
					for(i=0; i<DZFor_9CMDconst.GAME_PLAYER; i++)
					{
						if( DeZhouModel._getInstance().m_PlayerInfo[i] == userID )
						{
							wChaidID = i;
							break;
						}
					}
					if(wChaidID != 0)
					{
//						this['UV_' + wChaidID].userLeave();
					}
					return true;
				}
				
				if(cbStatus == TConst.US_NULL && wTableID == TConst.INVALID_CHAIR && wViewChairID == TConst.INVALID_CHAIR && !_theModel.m_gameStart)
				{
					for(i=0; i<DZFor_9CMDconst.GAME_PLAYER; i++)
					{
						if(userID == _theModel.m_PlayerInfo[i])
						{
							this['UV_' + i].visible = false;
							break;
						}
					}
					
					if(_theModel.GetUserNumber < 3)
					{
						showContinueBtn();
						
						TweenMax.delayedCall(StaticVars.delayedExitGame, exitGame);
						MsgManager.getInstance().showMessage1('游戏玩家已退出，请点击开始继续游戏！');
					}
				}
				
				
				
				if(cbStatus < TConst.US_SIT || cbStatus == TConst.US_LOOKON || wViewChairID >= DZFor_9CMDconst.GAME_PLAYER ) return true;
				if(cbStatus == TConst.US_OFFLINE)
				{
					this['UV_' + wViewChairID].Offlie(30000);
					//					if(_user.SwitchViewChairID(ZjhModel._getInstance().wCurrentUser) == wViewChairID) { 
					//						StopClock();
					//					}
					return true;
				}
				if(cbStatus > TConst.US_FREE)
				{
					this['UV_' + wViewChairID].Offlie(0);
				}
				return true;
			}
