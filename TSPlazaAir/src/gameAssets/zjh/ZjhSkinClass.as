		public static function GetCard(value : uint,w : uint = 78,h : uint = 107,cardType : String = 'look') : DisplayObject
		{
			var card : DisplayObject;
			if(cardType == 'look')
				card = new card_look();
			else if(cardType == 'lose')
				card = new card_lose();
			else if(cardType == 'lookdrop')
				card = new card_lookdrop();
			else if(cardType == 'lookcard1')
				card = new lookcard1();
			else if(cardType == 'lookcard2')
				card = new lookcard2();
			else if(cardType == 'losecard')
				card = new losecard();
			card.width = w;
			card.height = h;
			card.name = value.toString();
			return card;
		}
		
		public static function GetClock() : DisplayObject
