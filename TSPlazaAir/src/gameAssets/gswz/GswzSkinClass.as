		public static var showType:GswzSkinClass;
		private var typeAry:Array;
		
		
		public static function GetCowType(typeIndex:int) : DisplayObject
		{
			if(showType == null)	showType = new GswzSkinClass;
			return showType.getShowType(typeIndex-1);
		}
		
		
		private function getShowType(typeIndex:int):DisplayObject
		{
			if(typeAry == null)
				typeAry = [	
					cardType1, cardType2, cardType3, cardType4, cardType5,  
					cardType6, cardType7, cardType8, cardType9
				];
			return new typeAry[typeIndex];
		}
		
		
		
		
		
		
		
